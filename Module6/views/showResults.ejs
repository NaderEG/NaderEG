<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Colors</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].colors %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>
    <h2>Navbar</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].navbar %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Articles</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].articles %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>
    <h2>Quality</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].quality %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>
    <h2>Rating</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].guilty %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
    <h2>Comments</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].comments %> :  <%= results[5][i].count %></li>
    <% } %>    
    </ul>
</body>
</html>